{
  "name": "@comandeer/eslint-config",
  "version": "0.11.0",
  "type": "module",
  "description": "Shareable ESLint configuration",
  "exports": {
    ".": {
      "import": "./dist/eslint-config.mjs"
    },
    "./formatting": {
      "import": "./dist/eslint-config-formatting.mjs"
    }
  },
  "scripts": {
    "lint": "eslint \"{src,tests}/**/*.js\"",
    "build": "rlb",
    "test": "ava",
    "preversion": "npm test && npm run build",
    "postversion": "git push origin && git push origin --tags",
    "prepare": "husky install"
  },
  "engines": {
    "node": ">=16.12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Comandeer/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "author": "Comandeer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Comandeer/eslint-config/issues"
  },
  "homepage": "https://github.com/Comandeer/eslint-config#readme",
  "commitplease": {
    "nohook": true,
    "style": "angular",
    "types": [
      "feat",
      "fix",
      "docs",
      "style",
      "refactor",
      "perf",
      "test",
      "build",
      "ci",
      "chore",
      "revert"
    ],
    "scope": "\\S+.*"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "peerDependencies": {
    "eslint": ">=8.45.0"
  },
  "devDependencies": {
    "@comandeer/rollup-lib-bundler": "^0.21.0",
    "ava": "^5.3.1",
    "commitplease": "^3.2.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.45.0",
    "execa": "^7.1.1",
    "husky": "^8.0.3",
    "pathe": "^1.1.1",
    "tempy": "^3.1.0"
  },
  "dependencies": {
    "@babel/core": "^7.22.9",
    "@babel/eslint-parser": "^7.22.9",
    "@babel/eslint-plugin": "^7.22.5",
    "@babel/plugin-syntax-import-attributes": "^7.22.5",
    "@babel/preset-env": "^7.22.9",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "eslint-plugin-ava": "^14.0.0",
    "globals": "^13.20.0"
  }
}
